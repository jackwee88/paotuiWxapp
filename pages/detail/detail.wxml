		<view class="content">
			<view class="order">
				<view class="orderstatus">订单状态：{{status}}</view>
				<!-- 这里是总计 和 邮费 -->
				<view class="ordermoney">
					<view>订单编号：</view>
					<view>{{number}}</view>
				</view>
				<view class="ordermoney">
					<view>下单时间：</view>
					<view>{{createtime}}</view>
				</view>
				<view class="ordermoney">
					<view>支付时间：</view>
					<view>{{paytime}}</view>
				</view>
				<view class="ordermoney">
					<view>收件人：</view>
					<view>{{addressee}}</view>
				</view>
				<view class="ordermoney">
					<view>共支付</view>
					<view style="color:#E02020">￥{{money}}元</view>
				</view>

			</view>

		</view>
		<view class="footer-fixed" bindtap="confirm">
			确认收件
		</view>
		<view class="image-fixed">
			<image src="../../img/kf.png" style="width:60px;height:60px;"></image>
		</view>